<Window x:Class="InterfaceProjetBdd.SupprimerObjet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterfaceProjetBdd"
        mc:Ignorable="d"
        Title="SupprimerObjet" Height="450" Width="200">
    <Grid>
        <Button Name="RetourButton" Content="Retour" HorizontalAlignment="Center" Margin="0,382,0,0" VerticalAlignment="Top" Height="22" Width="124" Click="RetourButton_Click"/>
        <Button x:Name="Supprimer" Content="Supprimer" HorizontalAlignment="Center" Margin="0,340,0,0" VerticalAlignment="Top" Height="22" Width="124" Click="Supprimer_Click"/>
        <ComboBox x:Name="Object" HorizontalAlignment="Center" Margin="0,201,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="ComboBox_SelectionChanged">
        </ComboBox>
        <Button Name="FleurSelect" Content="Fleur" HorizontalAlignment="Center" Margin="0,117,0,0" VerticalAlignment="Top" Width="124" Click="FleurSelect_Click"/>
        <Button Name="AccessoireSelect" Content="Accessoire" HorizontalAlignment="Center" Margin="0,154,0,0" VerticalAlignment="Top" Width="124" Click="AccessoireSelect_Click"/>
    </Grid>
</Window>
