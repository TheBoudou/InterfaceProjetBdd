<Window x:Class="InterfaceProjetBdd.SuppressionClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterfaceProjetBdd"
        mc:Ignorable="d"
        Title="SuppressionClient" Height="250" Width="200">
    <Grid>
        <Button x:Name="RetourButton" Content="Retour" HorizontalAlignment="Center" Margin="0,131,0,0" VerticalAlignment="Top" Height="22" Width="128" Click="RetourButton_Click"/>
        <Button Name="Suppression" Content="Suppression" HorizontalAlignment="Center" Margin="0,93,0,0" VerticalAlignment="Top" Width="128" Click="Suppression_Click"/>
        <ComboBox x:Name="Object" HorizontalAlignment="Center" Margin="0,66,0,0" VerticalAlignment="Top" Width="129" SelectionChanged="ComboBox_SelectionChanged">
        </ComboBox>
        <TextBlock x:Name="Titre" HorizontalAlignment="Left" Margin="23,26,0,0" TextWrapping="Wrap" Text="Suppression Client" VerticalAlignment="Top" FontFamily="Arial" FontSize="18" Width="249"/>

    </Grid>
</Window>
